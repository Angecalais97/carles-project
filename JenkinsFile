pipeline{
  agent any

  tools {
    maven 'maven'
  }

  stages{
    stage ('scm'){
      steps{
        git branch: 'main', url: 'https://github.com/Angecalais97/carles-project'
      }
    }

    stage('build and test'){
      steps{
        sh ''' 
        mvn clean package
        '''
      }
    }
  }
}
